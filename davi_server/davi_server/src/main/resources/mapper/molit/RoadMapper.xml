<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.davi_server.molit.repository.RoadRepository">
    <select id="selectSampleList" resultType="com.davi_server.molit.dto.RoadDto">
        SELECT
            *
        FROM
            dv_molit_road
        limit 10
    </select>

    <select id="selectList" resultType="com.davi_server.molit.dto.RoadDto">
        SELECT
            *
        FROM
            dv_molit_road
        limit 1000
    </select>
</mapper>